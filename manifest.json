{

    "name": "Hide YouTube Live Chat",
    "author": "HexedHero",
    "version": "1.0.0",
    "description": "Removes YouTube's Live Chat Feature.",
    "manifest_version": 2,
    "content_scripts" :
    [

        {

            "matches" : ["*://*.youtube.com/*"],
            "js" : ["remove.js"]

        }

    ],
    "permissions":
    [

        "*://*.youtube.com/*"

    ],
    "icons":
    { 

        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"

    }

}